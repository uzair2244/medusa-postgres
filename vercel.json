{
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
    "routes": [
    {
        "src": "/(.*)",
        "dest": "./index.html",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
        "Access-Control-Allow-Origin": "*"
        }
    }
    ]
}